{
    "version": 2,
    "name": "eat-well-app",
    "builds": [
        { "src": "Front-end/mealhelper/package.json", "use": "@now/static-build", "config": { "distDir": "build" } }
    ],
    "routes": [
        { "src": "/(.*)", "headers": {"cache-control": "s-maxage=0"}, "dest": "/index.html" }
    ]
}