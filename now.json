{
    "name": "eat-well",
    "version": 2,
    "alias": "eat-well.now.sh",
    "builds": [
      { "src": "Front-end/mealhelper/package.json", 
        "use": "@now/static-build",
        "config": { "distDir": "build" } 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/front-end/$1"
      }
    ],
    "github": {
      "enabled": true
    }
  }
  